{
  "name": "SPFx-jesttest",
  "version": "0.0.1",
  "private": true,
  "engines": {
    "node": ">=0.10.0"
  },
  "scripts": {
    "build": "gulp bundle",
    "clean": "gulp clean",
    "generateAuth": "sp-auth init --path ./private.config.json",
    "test": "jest"
  },
  "dependencies": {
    "@microsoft/sp-core-library": "1.7.1",
    "@microsoft/sp-lodash-subset": "1.7.1",
    "@microsoft/sp-office-ui-fabric-core": "1.7.1",
    "@microsoft/sp-webpart-base": "1.7.1",
    "@pnp/common": "^1.3.1",
    "@pnp/graph": "^1.3.1",
    "@pnp/logging": "^1.3.1",
    "@pnp/odata": "^1.3.1",
    "@pnp/sp": "^1.3.1",
    "@types/es6-promise": "0.0.33",
    "@types/react": "16.4.2",
    "@types/react-dom": "16.0.5",
    "@types/webpack-env": "1.13.1",
    "react": "16.3.2",
    "react-dom": "16.3.2"
  },
  "resolutions": {
    "@types/react": "16.4.2"
  },
  "devDependencies": {
    "@microsoft/sp-build-web": "1.7.1",
    "@microsoft/sp-module-interfaces": "1.7.1",
    "@microsoft/sp-tslint-rules": "1.7.1",
    "@microsoft/sp-webpart-workbench": "1.7.1",
    "@pnp/nodejs": "^1.3.1",
    "@types/chai": "3.4.34",
    "@types/mocha": "2.2.38",
    "ajv": "~6.12.3",
    "chai": "^4.2.0",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.11.2",
    "gulp": "~3.9.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^24.5.0",
    "node-sp-auth-config": "^2.6.2",
    "pnp-auth": "0.0.4",
    "react-test-renderer": "^15.6.2",
    "sinon": "^7.2.7",
    "ts-jest": "^24.0.0"
  },
  "jest": {
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(ts?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ],
    "moduleNameMapper": {
      "\\.(css|scss)$": "identity-obj-proxy",
      "^resx-strings/en-us.json": "<rootDir>/node_modules/@microsoft/sp-core-library/lib/resx-strings/en-us.json",
      "^react/lib/React": "<rootDir>/node_modules/@microsoft/sp-core-library/lib/resx-strings/en-us.json"
    }
  }
}
